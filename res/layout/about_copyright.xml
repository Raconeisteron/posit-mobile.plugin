			android:layout_marginBottom="3dp"/>

		<ImageView android:src="@drawable/hfoss_logo" android:id="@+id/Logo" android:layout_height="match_parent" android:layout_width="wrap_content"></ImageView>

		<TextView 
			android:id="@+id/developmentTeamLabel"
			android:layout_width="wrap_content" 
			android:textSize="15sp" 
			android:textColor="#ffffff" 
