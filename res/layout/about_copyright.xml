			android:layout_centerHorizontal="true" android:gravity="center"/>

		<TextView
			android:id="@+id/developmentTeamLabel"
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content"
			android:textSize="15sp" 
			android:textColor="#ffffff" 
			android:text="@string/acdivocaDescription2"
			android:layout_marginBottom="3dp" 
			android:layout_centerHorizontal="true" android:gravity="center"/>

<ImageView android:layout_height="wrap_content" android:src="@drawable/acdivoca_logo" android:layout_width="wrap_content" android:id="@+id/Logo"></ImageView>

		<TextView
			android:id="@+id/developmentTeamLabel"
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content"
